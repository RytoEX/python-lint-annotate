{
  "problemMatcher": [
    {
      "code": "E",
      "pattern": [
        {
          "regexp": "^##\\[([a-z]*)\\](\\S*)(\\d+):(\\d+): ([a-z]+): (.*)$",
          "file": 2,
          "column": 4,
          "line": 3,
          "severity": 1,
          "message": 5
        }
      ],
      "owner": "mypy"
    }
  ]
}
